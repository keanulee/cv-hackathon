<script id="resume-template" type="text/x-handlebars-template">
  <li class="resume">
    <h1>{{ name }}</h1>
    <ul id="sections-list-{{ id }}">
    </ul>
    <a href="#" id="section-create-{{ id }}" data-type="text" data-pk="{{ id }}">Create Section</a> 
  </li>
</script>


<script id="section-template" type="text/x-handlebars-template">
  <li id="section-{{ id }}" class="section">
  

    <div class="accordion-heading section-header">
      <a data-toggle="collapse" data-parent="#accordion1" href="#collapse-{{ id }}" class="editable accordion-toggle" data-type="text" data-pk="{{ id }}" data-name="name" data-url="/sections/{{ id }}">{{ name }}</a>
    </div>
    
    <button class="delete-section" data="{{ id }}">Delete</button></h3>

    <!-- Change collapseOne to parts-list -->
    <div id="collapse-{{ id }}" class="accordion-body collapse out section">
      <div class="section-functionality">
        <h4>Section Settings</h4>

        <a href="#" class="delete-SECTION pull-right last" title="">
          <span class="livicon delete-section" data="{{ id }}" data-n="trash" data-s="32" data-c="#E74C3C" data-hc="#C0392B"data-et="click"></span>
        </a>

        <a href="#" class="edit-SECTION pull-right" title="">
          <span class="livicon" data-n="edit" data-s="32" data-c="#F1C40F" data-hc="#F39C12" data-et="click"></span>
        </a>

        <a href="#"  class="add-SECTION pull-right" title="">
          <span id="part-create-{{ id }}" data-type="text" data-pk="{{ id }}" class="livicon" data-n="plus" data-s="32" data-c="#2ECC71" data-hc="#27AE60" data-et="click"></span>
        </a>

      </div>
      <div class="accordion-inner section-info-container">
       <div class="accordion" id="parts-list-{{ id }}">
       
       </div>
      </div>
                   

    </div>

    
    <a href="#" id="part-create-{{ id }}" data-type="text" data-pk="{{ id }}">Create Part</a>

  </li>
</script>

<script id="part-template" type="text/x-handlebars-template">
  
    <div class="section-info">
      <div class="accordion-group info-area">

        <div class="accordion-heading info-header">
          
            <a href="#collapse-part-{{ id }}" data-toggle="collapse"  data-parent="#parts-list-{{ section_id }}" class="editable accordion-toggle" data-type="text" data-pk="{{ id }}" data-name="name" data-url="/parts/{{ id }}">{{ name }}</a>
        </div>

        <div id="collapse-part-{{ id }}" class="accordion-body collapse out info">
          <div class="info-functionality">
            <h4>Part Settings</h4>

            <a href="" class="delete-PART pull-right last" title="">
              <span class="livicon delete-part" data="{{ id }}" data-n="trash" data-s="32" data-c="#E74C3C" data-hc="#C0392B" data-et="click"></span>
            </a>

          </div> <!-- end info-functionality -->

          <div class="accordion-inner info-inner">
            <ul>
              <li><span class="field">Name: </span><span class="editable data" data-type="text" data-pk="{{ id }}" data-name="name" data-url="/parts/{{ id }}">{{ name }}</span></li>
              <li><span class="field">Location: </span><span class="editable data" data-type="text" data-pk="{{ id }}" data-name="location" data-url="/parts/{{ id }}">{{ location }}</span></li>
              <li><span class="field">Start Date: </span><span class="editable data" data-type="text" data-pk="{{ id }}" data-name="start_date" data-url="/parts/{{ id }}">{{start_date}}</span></li>
              <li><span class="field">End Date: </span><span class="editable data" data-type="text" data-pk="{{ id }}" data-name="end_date" data-url="/parts/{{ id }}">{{ end_date }}</span></li>
              <li><span class="field">Description: </span><span class="editable data" data-type="text" data-pk="{{ id }}" data-name="notes" data-url="/parts/{{ id }}">{{ notes }}</span></li>
            </ul>
          </div>

        </div> <!-- end .info -->

      </div> <!-- end info-area -->
    </div> <!-- end .section-info -->

